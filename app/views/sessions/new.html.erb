<h1>ログインページ</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  
    <%= f.label :"メールアドレス" %>
  <%= f.email_field :email %>
  <br>
  <br>
  
  <%= f.label :"パスワード" %>
  <%= f.password_field :password %>
  <br>
  <br>
  <div id= "create-session">
  <%= f.submit "ログイン" %>
  </div>
<% end %>